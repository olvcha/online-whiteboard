{"ast":null,"code":"import{io}from\"socket.io-client\";import{store}from\"../store/store\";import{setElements,updateElement}from\"../Whiteboard/whiteboardSlice\";let socket;export const connectWithSocketServer=()=>{socket=io(\"http://localhost:3003\");socket.on(\"console\",()=>{console.log(\"connected to socket.io server\");});socket.on(\"whiteboard-state\",elements=>{store.dispatch(setElements(elements));});socket.on(\"element-update\",elementData=>{store.dispatch(updateElement(elementData));});};export const emitElementUpdate=elementData=>{socket.emit(\"element-update\",elementData);};","map":{"version":3,"names":["io","store","setElements","updateElement","socket","connectWithSocketServer","on","console","log","elements","dispatch","elementData","emitElementUpdate","emit"],"sources":["/Users/ola/Repos/online-whiteboard/src/socketConn/socketConn.js"],"sourcesContent":["\nimport {io} from \"socket.io-client\"\nimport {store} from \"../store/store\"\nimport { setElements, updateElement } from \"../Whiteboard/whiteboardSlice\";\nlet socket;\n\nexport const connectWithSocketServer = () =>{\n    socket = io(\"http://localhost:3003\");\n\n    socket.on(\"console\", () =>{\n        console.log(\"connected to socket.io server\");\n    });\n\n    socket.on(\"whiteboard-state\", (elements) => {\n        store.dispatch(setElements(elements));\n    });\n\n    socket.on(\"element-update\", (elementData) => {\n        store.dispatch(updateElement(elementData));\n    })\n};\n\nexport const emitElementUpdate = (elementData) => {\n    socket.emit(\"element-update\", elementData);\n\n};\n"],"mappings":"AACA,OAAQA,EAAE,KAAO,kBAAkB,CACnC,OAAQC,KAAK,KAAO,gBAAgB,CACpC,OAASC,WAAW,CAAEC,aAAa,KAAQ,+BAA+B,CAC1E,GAAI,CAAAC,MAAM,CAEV,MAAO,MAAM,CAAAC,uBAAuB,CAAGA,CAAA,GAAK,CACxCD,MAAM,CAAGJ,EAAE,CAAC,uBAAuB,CAAC,CAEpCI,MAAM,CAACE,EAAE,CAAC,SAAS,CAAE,IAAK,CACtBC,OAAO,CAACC,GAAG,CAAC,+BAA+B,CAAC,CAChD,CAAC,CAAC,CAEFJ,MAAM,CAACE,EAAE,CAAC,kBAAkB,CAAGG,QAAQ,EAAK,CACxCR,KAAK,CAACS,QAAQ,CAACR,WAAW,